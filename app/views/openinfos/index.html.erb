<div class="oihead" >

  <h2>Selection of posts</h2>
  <p></p> 
</div>
<section class="openinformations">	
		
			<div class="createpublic" >
			<% if current_user? %>
			<%= link_to "Create", new_openinfo_path %>
			<% end %>
			</div>

			 <% @openinfos.each do |openinfo| %>
				
					<div class="openinformation">
						
						
							<h2><%= openinfo.title %></h2>
							<h5><%= openinfo.Openinformation %>
							<% if openinfo.Moreinformation.nil? %>
							</h5>
							<%else %>
										<%= link_to "....more.", openinfo_path(openinfo.id) %></h5>
							<% end %>
							<p></p>
							<% if current_user? %>
							<div class="Editingopen">
							<h5><%= link_to "Edit", edit_openinfo_path(openinfo.id) %></h5>
							<h5><%= link_to "Delete", openinfo_path(openinfo.id), method: :delete %></h5>
							</div>
							
							<% end %>	
				
					</div>
					
				<% end %> 

			

					
	</section>
