<%= simple_form_for [@user, @corporate], url: corporates_path, method: :post do |f| %>

	<%= f.input :name %>
	<%= f.input :country %>
	
	<%= f.input :sector %>
	<%= f.input :activity %>
	<%= f.input :address, label: 'Street, City' %>
	
	<%= f.input :urlevernote %>
	<%= f.input :urlcompanywebsite %>
	<%= f.input :Bloombergid %>
	<%= f.input :corpimage %>
	<%= f.input :logoimage %>
	
	

 
	
	<%= f.button :submit, "Add a corporate" %>
	* Required field
<% end %>