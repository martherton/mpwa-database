<div class="newopen" >
<h2>Edit Category</h2>

<%= simple_form_for [@user, @infocat], url: infocat_path, method: :put do |f| %>

	<h4>Title</h4><%= f.input :title, label: false %>
	<h4>Brief Description</h4><%= f.input :brief_description, label: false %>
	<h4>Item Order</h4><%= f.input :itemorder, label: false %>
	<h4>Routing</h4><%= f.input :routing, label: false %>

	<%= f.button :submit, "Add category" %>
	* Required field
<% end %>
</div>